/* pages/new/new.wxss */
.tit{
  position: relative;
}
.tit image,.tit swiper{
  width: 100%;
  height: 480rpx;
}
.img-math{
  position: absolute;
  right: 5%;
  bottom: 20rpx;
  height: 40rpx;
  line-height: 20rpx;
  padding: 0  20rpx;
  border-radius: 40rpx;
  background-color: #000;
  opacity: .5;
  text-align: center;
  z-index: 999;
}
.img-math-txt{
  line-height: 40rpx;
  color: #fff;
  font-size: 12px;
}
.pages-cont{
  padding: 0 20rpx;
}
.pages-cont .common-tit{
  text-align: center;
  padding: 10rpx 0;
}
.house-view{
  display: flex;
  flex-direction: row;
  margin: 10rpx 0;
  width: 100%;
  padding: 30rpx 0;
  border-top: 1px solid #E1E1E1;
  border-bottom: 1px solid #E1E1E1;
}
.house-view .layout-list{
  flex: 1;
  border-right: 1px solid #E1E1E1;
  text-align: center;
}
.house-view .layout-list:nth-last-child(1){
  border-right: none;
}
.house-view .layout-list .list-tags{
  position: relative;
  height: 32rpx;
  line-height: 32rpx;
  margin-bottom: 10rpx;
  color: #666;
  font-size: 12px;
}
.house-view .layout-list .list-math{
  color: #e75c61;
  font-size: 15px;
  font-weight: bold;
}
.house-more{
  text-align: center;
}